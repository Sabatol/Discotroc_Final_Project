<!DOCTYPE html>
<html>
  <head>
    <title>Discotroc</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <!-- Loading Bootstrap -->
    <link href="Discotroc_KitUI/dist/css/vendor/bootstrap.min.css" rel="stylesheet">

    <!-- Loading Flat UI -->
    <link href="Discotroc_KitUI/dist/css/flat-ui.css" rel="stylesheet">
    <link href="Discotroc_KitUI/docs/assets/css/demo.css" rel="stylesheet">

    <link rel="shortcut icon" href="dist/favicon.ico">

  </head>

  <body>
    <%= render 'layouts/navbar' %>

    <%= yield %>
    <%= render 'layouts/footer' %>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->

    <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>

    <!-- Bootstrap 4 requires Popper.js -->

    <script src="https://unpkg.com/popper.js@1.14.1/dist/umd/popper.min.js" crossorigin="anonymous"></script>
    <script src="http://vjs.zencdn.net/6.6.3/video.js"></script>
    <script src="Discotroc_KitUI/dist/scripts/flat-ui.js"></script>
    <script src="Discotroc_KitUI/docs/assets/js/application.js"></script>

  </body>
</html>