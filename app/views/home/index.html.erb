<p>
  <% artist = @wrapper.get_artist("329937")%>
  <%= artist.name%>
  <br>
  <%= artist.profile%>
  <br>
</p>
<%= form_tag(root_path,method: :get) do %>
  <%= text_field_tag(:search,params[:search])%>
  <%= submit_tag "search"%>
  <% @search.results.each do |search|%>
    <div class="card my-3" style="width: 18rem;">
      <img src="<%=search.cover_image %>">
      <div class="card-body">
        <h5 class="card-title"><%=search.title%></h5>
        <p class="card-text"><%=search.tracklist%></p>
        <a href="#" class="btn btn-primary">Go somewhere</a>
      </div>
    </div>
  <% end %>
  <ul>
    <% if user_signed_in? %>
      <li>
        <%= link_to "Sign out", destroy_user_session_path, method: :delete %>
      </li>
      <li>
        Voici ton email :
        <%= current_user.email %>
      </li>
      <li>
        <%= link_to "Secret page", root_path %>
      </li>
      <li>
        <%= link_to "Edit email / password", edit_user_registration_path %>
      </li>
    <% else %>
      <li>
        <%= link_to "Sign in", new_user_session_path %>
      </li>
      <li>
        <%= link_to "Sign up", new_user_registration_path %>
      </li>
    <% end %>
    <br>
    <br>
    <br>
    <br>
    <center>
      <%= link_to "Voir les disques", discs_path%>
      <br>
      <%= link_to "Voir les tracks", tracks_path%>
      <br>
      <%= link_to "Voir les styles", styles_path%>
      <br>
      <%= link_to "Voir les genres", genres_path%>
      <br>
      <%= link_to "Voir les artistes", artists_path%>
      <br>
      <%= link_to "Voir les deals", deals_path%>
      <br>
      <%= link_to "Voir les deals PM", deal_pms_path%>
      <br>
      <%= link_to "Voir les deals contents", deal_contents_path%>
      <br>
    </center>
  </ul>
<% end %>
